<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.employee.model.Employees" table="employee_info">
		<id name="EmployeeID" type="integer" column="EmployeeID">
			<generator class="increment" />

		</id>
		<property name="firstname" type="string" column="firstname"
			not-null="true" />
		<property name="surname" type="string" column="surname"
			not-null="true" />
		<property name="address" type="string" column="address"
			not-null="true" />
		<property name="email" type="string" column="email" not-null="true" />
		<property name="phone" type="integer" column="phone"
			not-null="true" />
		<property name="date_of_birth" type="string" column="date_of_birth"
			not-null="true" />
		<property name="gender" type="string" column="gender"
			not-null="true" />
		<property name="salary" type="integer" column="salary"
			not-null="true" />
	</class>
</hibernate-mapping>
